<mvc:View controllerName="ztestproject2.controller.View1" xmlns:form="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <form:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="3" emptySpanXL="5">
                <form:toolbar>
                    <OverflowToolbar >
                        <ToolbarSpacer ></ToolbarSpacer>
                        <Button text="Navigate" press="onPressNav"></Button>
                        <Button text="Go" press="onPressGo" ></Button>
                    </OverflowToolbar>
                </form:toolbar>
                <form:content>
                    <Label text="Multi Input Field"></Label>

                    <MultiInput id="idMultiInput" ></MultiInput>
                </form:content>
            </form:SimpleForm>
        </content>
    </Page>
</mvc:View>
